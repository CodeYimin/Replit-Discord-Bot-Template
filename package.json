{
  "name": "replit-discord-bot-template",
  "author": {
    "name": "Yimin"
  },
  "version": "1.0.0",
  "description": "Discord bot template for use on repl.it",
  "main": "src/main.ts",
  "license": "MIT",
  "scripts": {
    "dev": "concurrently \"nodemon\" \"npm run prisma:dev\"",
    "prisma:dev": "nodemon -e prisma -w prisma -x \"npx prisma db push --accept-data-loss && npx prisma generate && npx prisma studio --browser none\"",
    "run:dist": "node ./dist/main.js",
    "build": "tsc --build",
    "prod": "npx prisma db push && npm run build && npm run run:dist"
  },
  "dependencies": {
    "@discordx/importer": "^1.1.10",
    "@discordx/pagination": "^2.2.2",
    "@discordx/utilities": "^4.3.3",
    "@prisma/client": "^4.0.0",
    "axios": "^0.27.2",
    "discord.js": "^13.8.1",
    "discordx": "^9.8.0",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "lodash": "^4.17.21",
    "reflect-metadata": "^0.1.13"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/lodash": "^4.14.182",
    "@types/node": "^18.0.3",
    "@typescript-eslint/eslint-plugin": "^5.30.5",
    "@typescript-eslint/parser": "^5.30.5",
    "concurrently": "^7.2.2",
    "eslint": "^8.19.0",
    "nodemon": "^2.0.19",
    "prisma": "^4.0.0",
    "ts-node": "^10.8.2",
    "typescript": "^4.7.4"
  }
}
